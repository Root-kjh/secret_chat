<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="login" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="500.0" prefWidth="350.0"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="login.LoginController">
	<children>
		<AnchorPane prefHeight="460.0" prefWidth="281.0"
			stylesheets="/index/index.css">
			<children>
				<TextField styleClass="input" fx:id="ID" layoutX="45.0" layoutY="193.0"
					prefHeight="33.0" prefWidth="261.0" promptText="아이디">
					<font>
						<Font size="18.0" />
					</font>
				</TextField>
				<PasswordField styleClass="input" fx:id="password" layoutX="45.0"
					layoutY="234.0" prefHeight="33.0" prefWidth="261.0"
					promptText="비밀번호">
					<font>
						<Font size="18.0" />
					</font>
				</PasswordField>
				<Button fx:id="submit" layoutX="45.0" layoutY="276.0"
					mnemonicParsing="false" prefHeight="32.0" prefWidth="261.0"
					text="로그인" />

				<Button fx:id="go" layoutX="45.0" layoutY="316.0"
					mnemonicParsing="false" onAction="#Sign_UP" prefHeight="32.0"
					prefWidth="261.0" text="회원가입" />
			</children>
		</AnchorPane>
	</children>
</StackPane>
