<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.PasswordField?>

<AnchorPane prefHeight="550" prefWidth="350"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="login.LoginController">
	<children>
		<TextField fx:id="textUser" layoutX="76" layoutY="224"
			promptText="ID">
			<font>
				<Font size="18.0" />
			</font>
		</TextField>
		<PasswordField  fx:id="textPassword" layoutX="76" layoutY="260"
			promptText="password">
			<font>
				<Font size="18.0" />
			</font>
		</PasswordField>
		<Button layoutX="76" layoutY="300" prefHeight="33.0"
			prefWidth="104.0" text="Login">
			<font>
				<Font size="18.0" />
			</font>
		</Button>
		<Button layoutX="180" layoutY="300" prefHeight="33.0"
			prefWidth="104.0" text="Sign up">
			<font>
				<Font size="18.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
